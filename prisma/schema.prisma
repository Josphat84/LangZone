generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Instructor {
  id          Int      @id @default(autoincrement())
  name        String
  email       String   @unique
  language    String
  expertise   String
  price       Float
  description String
  country     String
  is_native   Boolean
  image_url   String?
  createdAt   DateTime @default(now())
}

